{
  "name": "@paima/standalone",
  "author": "",
  "license": "ISC",
  "version": "1.0.0",
  "description": "Standalone usage of paima-engine meant as an executable wrapper around the engine",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "files": [
    "/build"
  ],
  "bin": "packaged/engineCorePacked.js",
  "pkg": {
    "scripts": [],
    "assets": [
      "packaged/*.wasm"
    ],
    "targets": [
      "node18-linux",
      "node16-linux"
    ],
    "outputPath": "packaged"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./esbuildconfig.cjs",
    "build:binary": "npm run build && npx pkg packaged/engineCorePacked.js --options experimental-specifier-resolution=node --config package.json"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.4"
  }
}
