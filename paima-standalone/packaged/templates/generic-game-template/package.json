{
  "name": "@game/template-game",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "initialize": "git init ../paima-sdk && npm --prefix ../paima-sdk install && npm ci && cp .env.example ../.env.development",
    "pack": "sh pack.sh",
    "build": "npx tsc --build tsconfig.build.json",
    "lint": "eslint --ext ts,tsx --ignore-path .gitignore .",
    "lint:fix": "eslint --ext ts,tsx --ignore-path .gitignore --fix .",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --check .",
    "prettier:fix": "prettier --write --config .prettierrc --ignore-path .prettierignore --check .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "npm test -- --watch"
  },
  "workspaces": [
    "./utils",
    "./db",
    "./api",
    "./game-logic",
    "./backend",
    "./middleware"
  ],
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^17.0.31",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "paima-sdk": "file:../paima-sdk"
  }
}
