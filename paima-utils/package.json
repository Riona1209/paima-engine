{
  "name": "@paima/utils",
  "version": "1.0.0",
  "description": "Library for Paima helper functions",
  "main": "build/index.js",
  "type": "module",
  "types": "build/index.d.ts",
  "export": {
    ".": {
      "types": "./build/index.d.ts"
    }
  },
  "files": [
    "/build"
  ],
  "scripts": {
    "prebuild": "npm run generate-types",
    "compile": "npx pgtyped -w -c pgtypedconfig.json",
    "build": "tsc",
    "generate-types": "npx typechain --target=web3-v1 'src/artifacts/*.json' --out-dir src/contract-types",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier --write '**/*.ts'"
  },
  "devDependencies": {
    "@typechain/web3-v1": "^6.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "typechain": "^8.1.1"
  },
  "dependencies": {
    "@pgtyped/query": "1.0.1"
  }
}
